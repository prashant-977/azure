{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dataingestion-dev-pm001"
		},
		"LS_abs_buyolot_webstore_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_abs_buyolot_webstore'"
		},
		"LS_abs_webstore_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_abs_webstore'"
		},
		"LS_adls_data_engineering_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_adls_data_engineering_dl'"
		},
		"LS_adls_data_engineering_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdededatalakedev001.dfs.core.windows.net/"
		},
		"TRG_ingest_uploaded_file_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/866ecae6-6a3b-43de-b026-c702bd198d9c/resourceGroups/rg-webdevelopment-dev-pm001/providers/Microsoft.Storage/storageAccounts/hypolwebstoredata001"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dataingestion-dev-pm001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_abs_buyolot_webstore_connectionString": {
						"value": "[parameters('LS_abs_buyolot_webstore_connectionString')]"
					},
					"LS_abs_webstore_connectionString": {
						"value": "[parameters('LS_abs_webstore_connectionString')]"
					},
					"LS_adls_data_engineering_dl_accountKey": {
						"value": "[parameters('LS_adls_data_engineering_dl_accountKey')]"
					},
					"LS_adls_data_engineering_dl_properties_typeProperties_url": {
						"value": "[parameters('LS_adls_data_engineering_dl_properties_typeProperties_url')]"
					},
					"TRG_ingest_uploaded_file_properties_typeProperties_scope": {
						"value": "[parameters('TRG_ingest_uploaded_file_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}